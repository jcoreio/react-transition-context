{
  "name": "react-redux-features",
  "version": "0.0.0-development",
  "description": "React components for redux-features",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test stories",
    "lint:fix": "eslint --fix src test stories",
    "lint:watch": "esw --watch src test stories",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore lib/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "build": "rimraf lib && babel src --out-dir lib && flow-copy-source -v src/ lib",
    "test": "NODE_ENV=production BABEL_ENV=test nyc --reporter=lcov --reporter=text mocha $npm_package_config_mocha",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run lint && flow",
    "prepush": "npm test",
    "prepublish": "npm run lint && flow && npm test && npm run build && flow-copy-source -v src/ lib",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "mocha": "--compilers js:babel-core/register -r jsdom-global/register ./test/**.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/react-redux-features.git"
  },
  "keywords": [
    "redux-features",
    "react",
    "feature-oriented programming",
    "code splitting",
    "fop"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/react-redux-features/issues"
  },
  "homepage": "https://github.com/jcoreio/react-redux-features#readme",
  "devDependencies": {
    "@jedwards1211/eslint-config-flow": "^1.0.0",
    "@jedwards1211/eslint-config-react": "^2.0.1",
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.0.0",
    "copy": "^0.3.0",
    "coveralls": "^2.11.15",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "flow-bin": "^0.56.0",
    "flow-copy-source": "^1.2.1",
    "flow-watch": "^1.1.0",
    "history": "^4.7.2",
    "husky": "^0.14.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.8.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "nyc": "^11.3.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-features": "^3.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^7.0.1",
    "sinon": "^2.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0 || ^16",
    "redux-features": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.0",
    "react-redux": "^5.0.6",
    "reselect": "^3.0.0"
  }
}
